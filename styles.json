{
    "container": {
        "flex": 1,
        "backgroundColor": "#263238",
        "flexDirection": "column"
    },
    "view": {
        "flex": 1,
        "backgroundColor": "#fdd835",
        "alignItems": "center",
        "justifyContent": "center"
    },
    "view2": {
        "flex": 1,
        "backgroundColor": "#263238",
        "alignContent": "center",
        "alignItems": "center",
        "justifyContent": "center"
    },
    "input": {
        "borderBottomColor": "#4f5b62",
        "borderBottomWidth": 3,
        "color": "white",
        "margin": 10
    },
    "buttonContainer": {
        "flexDirection": "row",
        "flex": 1,
        "backgroundColor": "#263238",
        "justifyContent": "space-evenly",
        "flexWrap": "wrap"
    },
    "flatListContainer": {
        "flexDirection": "column",
        "flex": 9,
        "backgroundColor": "#263238",
        "justifyContent": "space-around",
        "flexWrap": "wrap"
    },
    "text": {
        "fontSize": 3
    },
    "image": {
        "flex": 0.3,
        "margin": 20,
        "height": 50,
        "resizeMode": "stretch"
    }
}